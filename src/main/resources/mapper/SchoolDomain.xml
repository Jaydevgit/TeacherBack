<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.teacher.scholat.dao.SchoolDomain">
    <select id="GetSchoolDomain" resultType="com.alibaba.fastjson.JSONObject">
        SELECT s.school_domain
        FROM school_domain s
        WHERE s.school_name =  #{school_name}
    </select>
</mapper>